  "repository": {
    "type": "git",
    "url": "https://github.com/urqw/UrqW.git"
  },
  "homepage": "https://urqw.github.io/UrqW",
  "bugs": {
    "url": "https://github.com/urqw/UrqW/issues"
  },
  "keywords": [
