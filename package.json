{
  "name": "urqw",
  "version": "1.1.0-beta",
  "description": "Free and open source text-based game engine in JavaScript.",
  "main": "index.html",
  "directories": {
    "doc": "md"
  },
  "scripts": {
    "check-locale": "node scripts/check-locale.js",
    "gen-docs": "node scripts/gen-docs.js",
    "gen-license-report": "node scripts/gen-license-report.js",
    "gen-rss": "node scripts/gen-rss.js",
    "pack-games": "node scripts/pack-games.js",
    "valid-json": "node scripts/valid-json.js",
    "pack-release": "node scripts/pack-release.js",
    "build": "npm run valid-json && npm run pack-games && npm run gen-rss && npm run gen-license-report && npm run gen-docs",
    "release": "npm run gen-license-report && npm run gen-docs && npm run pack-release",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Akela <tinycoyote@gmail.com>",
  "contributors": [
    "Nikita Tseykovets <tseikovets@rambler.ru>"
  ],
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/urqw/UrqW.git"
  },
  "homepage": "https://urqw.github.io/UrqW",
  "bugs": {
    "url": "https://github.com/urqw/UrqW/issues"
  },
  "keywords": [
    "interactive-fiction",
    "text-based-game-engine",
    "text-adventure-engine",
    "urq",
    "parser",
    "interpreter"
  ],
  "dependencies": {
    "bootstrap": "^3.4.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "jquery": "^2.1.4",
    "jszip": "^2.7.0",
    "jszip-utils": "^0.0.2"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "license-checker": "^25.0.1",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-table-of-contents": "^0.9.0",
    "moment": "^2.30.1"
  }
}
