{
  "name": "urqw",
  "version": "1.1.0-beta",
  "description": "Open source URQ games interpreter in JavaScript.",
  "main": "index.html",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "gen-license-report": "node scripts/gen-license-report.js",
    "gen-rss": "node scripts/gen-rss.js",
    "pack-games": "node scripts/pack-games.js",
    "valid-json": "node scripts/valid-json.js",
    "build": "npm run valid-json && npm run pack-games && npm run gen-rss && npm run gen-license-report",
    "test": "echo \"Error: no test specified\" && exit 1"
